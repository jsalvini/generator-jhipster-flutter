import 'package:flutter/material.dart';
import 'package:go_router/go_router.dart';
import 'package:<%= baseName %>/core/router/routes.dart';
import 'package:<%= baseName %>/splash/views/splash_screen.dart';

class SplashRoutes {
  static const path = '/splash';
  static const screenKey = Key('__splashScreen__');

  static final splashRoutes = <GoRoute>[
    GoRoute(
      name: <%= camelizedUpperFirstBaseName %>Routes.splash,
      path: path,
      pageBuilder: (context, state) => const MaterialPage(
        child: SplashScreen(),
      ),
    ),
  ];
}
